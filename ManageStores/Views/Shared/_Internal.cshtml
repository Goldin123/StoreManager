<!DOCTYPE html>

<html lang="en">

<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>
    <!-- Google Font: Source Sans Pro -->
    <link rel="stylesheet" href="https://fonts.googleapis.com/css?family=Source+Sans+Pro:300,400,400i,700&display=fallback">
    <!-- Font Awesome Icons -->
    <link href="~/Content/plugins/fontawesome-free/css/all.min.css" rel="stylesheet" />
    <!-- overlayScrollbars -->
    <link href="~/Content/plugins/overlayScrollbars/css/OverlayScrollbars.min.css" rel="stylesheet" />
    <!-- Theme style -->
    <link href="~/Content/dist/css/adminlte.min.css" rel="stylesheet" />

    <link href="~/Content/plugins/sweetalert2-theme-bootstrap-4/bootstrap-4.min.css" rel="stylesheet" />
    <link rel="stylesheet" href="https://cdn.datatables.net/1.13.4/css/jquery.dataTables.css" />
    <!-- jQuery -->
    <script src="~/Content/plugins/jquery/jquery.min.js"></script>
    <script src="https://cdn.datatables.net/1.13.4/js/jquery.dataTables.js"></script>
</head>

<body class="hold-transition dark-mode sidebar-mini layout-fixed layout-navbar-fixed layout-footer-fixed">
    <style>
        .card-body {
            width: 900px
        }
    </style>
    <div class="wrapper">

        <!-- Preloader -->
        <div class="preloader flex-column justify-content-center align-items-center">
        </div>

        <!-- Navbar -->
        <nav class="main-header navbar navbar-expand navbar-dark">
            <!-- Left navbar links -->
            <ul class="navbar-nav">
                <li class="nav-item">
                    <a class="nav-link" data-widget="pushmenu" href="#" role="button"><i class="fas fa-bars"></i></a>
                </li>
                <li class="nav-item d-none d-sm-inline-block">
                    <a href="/" class="nav-link">Home</a>
                </li>
            </ul>


        </nav>
        <!-- /.navbar -->
        <!-- Main Sidebar Container -->
        <aside class="main-sidebar sidebar-dark-primary elevation-4">

            <!-- Sidebar -->
            <div class="sidebar">
                <!-- Sidebar user panel (optional) -->
                <div class="user-panel mt-3 pb-3 mb-3 d-flex">
                    <div class="info">
                        @*<p>@GlodinAutoTradeModel.Models.Globals.Name</p>*@
                    </div>
                </div>
                <!-- Sidebar Menu -->
                <nav class="mt-2">
                    <ul class="nav nav-pills nav-sidebar flex-column" data-widget="treeview" role="menu" data-accordion="false">
                        <li class="nav-item" role="presentation">
                            <a href="@Url.Action("Index","Customer")" class="nav-link" role="menuitem">
                                <i class="nav-icon fas fa-smile"></i>
                                <p>
                                    Profile
                                </p>
                            </a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a href="@Url.Action("Index","Product")" class="nav-link" role="menuitem">
                                <i class="nav-icon fas fa-list-alt"></i>
                                <p>
                                    Products
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview" role="presentation">
                                <li class="nav-item" role="presentation">
                                    <a href="@Url.Action("Index","Product")" class="nav-link" role="menuitem">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>
                                            View
                                            @*<span class="right badge badge-success">(@GlodinAutoTradeModel.Models.Globals.TotalProducts) product(s)</span>*@
                                        </p>
                                    </a>
                                </li>
                                <li class="nav-item" role="presentation">
                                    <a href="@Url.Action("AddProduct","Product")" class="nav-link" role="menuitem">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Add</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="nav-item " role="presentation">
                            <a href="@Url.Action("Index","Store")" class="nav-link " role="menuitem">
                                <i class="nav-icon fas fa-building"></i>
                                <p>
                                    Stores
                                    <i class="right fas fa-angle-left"></i>
                                </p>
                            </a>
                            <ul class="nav nav-treeview" role="presentation">
                                <li class="nav-item" role="presentation">
                                    <a href="@Url.Action("Index","Store")" class="nav-link" role="menuitem">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>
                                            Manage
                                            @*<span class="right badge badge-success">(@GlodinAutoTradeModel.Models.Globals.TotalProducts) product(s)</span>*@
                                        </p>
                                    </a>
                                </li>
                                <li class="nav-item" role="presentation">
                                    <a href="@Url.Action("AddStore","Store")" class="nav-link" role="menuitem">
                                        <i class="far fa-circle nav-icon"></i>
                                        <p>Add</p>
                                    </a>
                                </li>

                            </ul>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a href="@Url.Action("about","Home")" class="nav-link" role="menuitem">
                                <i class="nav-icon fas fa-calendar"></i>
                                <p>
                                    History

                                </p>
                            </a>
                        </li>
                        <li class="nav-item" role="presentation">
                            <a href="@Url.Action("signout","Account")" class="nav-link" role="menuitem">
                                <i class="nav-icon fas fa-bolt"></i>
                                <p>
                                    Sign Out
                                    <span class="right badge badge-warning">OFF</span>
                                </p>
                            </a>
                        </li>

                    </ul>
                </nav>
                <!-- /.sidebar-menu -->
            </div>
            <!-- /.sidebar -->
        </aside>

        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- /.content-header -->
            <!-- Main content -->
            <section class="content">
                <div class="container-fluid">
                    <!-- Info boxes -->
                    <div class="row">
                        @RenderBody()
                    </div>
                    <!-- /.row -->

                </div><!--/. container-fluid -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <!-- Control Sidebar -->
        <aside class="control-sidebar control-sidebar-dark">
            <!-- Control sidebar content goes here -->
        </aside>
        <!-- /.control-sidebar -->
        <!-- Main Footer -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2023 </strong>
            Goldin Store Manager 1.0
        </footer>
    </div>
    <!-- ./wrapper -->
    <!-- REQUIRED SCRIPTS -->
    <!-- Bootstrap -->
    <script src="~/Content/plugins/bootstrap/js/bootstrap.bundle.min.js"></script>


    <script src="~/Content/plugins/jquery.unobtrusive-ajax.min.js"></script>
    <script src="~/Content/plugins/jquery.validate.min.js"></script>
    <script src="~/Content/plugins/jquery.validate.unobtrusive.min.js"></script>
    <!-- overlayScrollbars -->
    <script src="~/Content/plugins/overlayScrollbars/js/OverlayScrollbars.min.js"></script>

    <script src="~/Content/plugins/bs-custom-file-input/bs-custom-file-input.min.js"></script>

    <!-- AdminLTE App -->
    <script src="~/Content/dist/js/adminlte.js"></script>

    <!-- PAGE PLUGINS -->
    <!-- jQuery Mapael -->
    <script src="~/Content/plugins/jquery-mousewheel/jquery.mousewheel.js"></script>
    <script src="~/Content/plugins/raphael/raphael.min.js"></script>
    <script src="~/Content/plugins/jquery-mapael/jquery.mapael.min.js"></script>
    <script src="~/Content/plugins/sweetalert2/sweetalert2.min.js"></script>
</body>

</html>
